    style.
      body {
        font-size: 140%;
        font-family: "Raleway", sans-serif; 
      }
    style.
      #myCanvas {
      border: 1px solid #9C9898;
      }
      #myCanvas2 {
      border: 1px solid #9C9898;
      }
      #myCanvas3 {
      border: 1px solid #9C9898;
      }
