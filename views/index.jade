  body(ng-controller='indexController')
    header
      div
        // Nav tabs
        ul.nav.nav-tabs(role='tablist')
          li.active(role='presentation')
            a(href='#home', aria-controls='home', role='tab', data-toggle='tab') Inicio
          li(role='presentation')#showModal
            a(href='#registrar', aria-controls='profile', role='tab', data-toggle='tab') Registar Historia Clinica
          li(role='presentation')
            a(href='#search', aria-controls='messages', role='tab', data-toggle='tab') Busqueda de Historia Clinica
          li(role='presentation')
            a(href='#queue', aria-controls='settings', role='tab', data-toggle='tab') Pacientes en Espera
        // Tab panes
        .tab-content
          #home.tab-pane.fade.in.active(role='tabpanel') 
            .container-fluid
              .jumbotron(style='margin-top:1%')
          #registrar.tab-pane.fade(role='tabpanel')
            .container-fluid
              .jumbotron(style='margin-top:1%')
                #accordion.panel-group(role='tablist', aria-multiselectable='true')
